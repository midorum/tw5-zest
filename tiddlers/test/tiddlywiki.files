	"directories": [
		{
			"path": ".",
			"filesRegExp": "^.*\\.js$",
			"isTiddlerFile": true,
			"fields": {
				"tags": "$:/tags/test-spec",
				"title": {
					"source": "filename",
					"prefix": "test/"
				},
				"type": "application/javascript"
			}
		},
		{
			"path": "./service",
			"filesRegExp": "^.*$",
			"isTiddlerFile": true,
			"fields": {
